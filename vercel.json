{
  "version": 2,
  "builds": [
    { "src": "dist/my-Task/browser/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/dist/my-Task/browser/assets/$1" },
    { "src": "/favicon.ico", "dest": "/dist/my-Task/browser/favicon.ico" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))", "dest": "/dist/my-Task/browser/$1" },
    { "src": "/(.*)", "dest": "/dist/my-Task/browser/index.html" }
  ]
}
